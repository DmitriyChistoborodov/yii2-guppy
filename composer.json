{
  "name": "xbrodies/yii2-guppy",
  "version" : "1.0",
  "type": "yii2-extension",

  "description": "Easy & Fast CMS based Modx",
  "keywords": ["yii2", "cms", "guppy"],
  "license": "BSD-3-Clause",
  "support": {

  },
  "authors": [
    {
      "name": "Dmitriy Chistoborodov",
      "email": "skillet123q@gmail.com"
    }
  ],

  "require": {
    "yiisoft/yii2": "~2.0.0",
    "dektrium/yii2-user": "^0.9.0@dev",
    "dektrium/yii2-rbac": "1.0.0-alpha@dev"
  },

  "config": {
    "process-timeout": 1800,
    "fxp-asset":{
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  },

  "extra": {
    "branch-alias": {
      "dev-master": "1.11.x-dev"    }
  },

  "autoload": {
    "psr-4": {
      "xbrodies\\guppy\\": "src/"
    }
  }
}
